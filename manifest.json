{
  "manifest_version": 2,
  "name": "Programmer Alarm Clock",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "js/content.js" ]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "http://pac.su/*"
  ],

      "chrome_url_overrides":
    {
        "newtab": "html/popup.html"
    },
	
}